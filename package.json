{
  "name": "multiformat-multicodec",
  "version": "1.0.0",
  "description": "An interface for (de)encoding a wide range of multicodecs simultaneously",
  "types": "dist/index.d.ts",
  "source": "src/index.ts",
  "main": "dist/index.js",
  "unpkg": "dist/index.umd.js",
  "module": "dist/index.mjs",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "default": "./dist/index.mjs"
    }
  },
  "scripts": {
    "build": "microbundle --compress=false"
  },
  "keywords": [
    "multiformat",
    "multicodec",
    "ipld",
    "decoder",
    "encoder",
    "codec"
  ],
  "author": "Upkeep",
  "license": "MIT",
  "devDependencies": {
    "microbundle": "^0.15.1",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "multiformats": "^9.7.1"
  }
}
